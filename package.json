{
  "name": "react-grain",
  "private": false,
  "version": "0.1.3",
  "type": "module",
  "license": "MIT",
  "description": "A React based Grain Component",
  "author": "Surya surya-teja-222",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src/assets"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-grain.es.js",
      "require": "./dist/react-grain.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/index.d.ts": "./dist/index.d.ts",
    "./dist/*": "./dist/*"
  },
  "main": "./dist/react-grain.umd.js",
  "module": "./dist/react-grain.es.js",
  "types": "./dist/index.d.ts",
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.5.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-linter": "^2.1.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
